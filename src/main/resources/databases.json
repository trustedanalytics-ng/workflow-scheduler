[
  {
    "name": "PostgreSQL",
    "drivers": [
      {
        "name": "postgresql",
        "version": "9.3",
        "className": "org.postgresql.Driver",
        "jdbcRegex" : "jdbc:postgresql:\/\/([\\w:._-]+):([1-9][0-9]{0,4})\/([\\w]+)",
        "jdbcTemplate": "jdbc:postgresql://{host}:{port}/{database}"
      }
    ],
    "schema" : {
      "supported": true,
      "required" : false
    }

  }
]
